<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wotin.geniustest">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".Activity.Test.TestQuicknessActivity"></activity>
        <activity android:name=".Activity.Test.TestConcentractionActivity" />
        <activity android:name=".Activity.Test.TestActivity" />
        <activity android:name=".Activity.UserManagement.UserInformationActivity" />
        <activity
            android:name=".Activity.Practice.PracticeQuicknessActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.Practice.PracticeConcentractionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.UserManagement.DeleteUserActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.LoginAndSignUp.SignUpActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.LoginAndSignUp.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.GeniusTestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.Practice.PracticeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.IntroActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>